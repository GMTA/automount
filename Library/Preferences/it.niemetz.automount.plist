<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CommonMaxRetryInSeconds</key>
	<integer>COMMONMAXRETRYINSECONDS (optional, eg. 30)</integer>
	<key>CommonValidIPRanges</key>
	<string>COMMONVALIDIPRANGES (optional, separated by comma, eg. 10.0.0,192.168.0)</string>
	<key>CommonAccount</key>
	<string>COMMONACCOUNT (optional)</string>
	<key>CommonMountOptions</key>
	<string>COMMONMOUNTOPTIONS (optional, separated by comma, eg. nodev,nosuid)</string>
	<key>Mountlist</key>
	<array>
		<dict>
			<key>MaxRetryInSeconds</key>
			<integer>MAXRETRYINSECONDS (optional)</integer>
			<key>ValidIPRanges</key>
			<string>VALIDIPRANGES (optional, separated by comma, eg. 10.0.0,192.168.0)</string>
			<key>MountOptions</key>
			<string>MOUNTOPTIONS (optional)</string>
			<key>Protocol</key>
			<string>PROTOCOL (mandatory, possible values are afp/smb/ftp/nfs/http/https)</string>
			<key>Domain</key>
			<string>DOMAIN (optional)</string>
			<key>Account</key>
			<string>ACCOUNT (optional)</string>
			<key>Server</key>
			<string>SERVER (mandatory)</string>
			<key>Share</key>
			<string>SHARE (mandatory)</string>
			<key>MountPoint</key>
			<string>MOUNTPOINT (optional)</string>
		</dict>
	</array>
</dict>
</plist>
